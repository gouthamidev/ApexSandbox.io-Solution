public String aOrAn(String word) {
    //code here

 string result;
        string vowel1 = 'a';
        string vowel2 = 'e';
        string vowel3 = 'i';
        string vowel4 = 'o';
        string vowel5 = 'u';
        if(word.startsWith(vowel1) || word.startsWith(vowel2) || word.startsWith('i') || word.startsWith('o') || word.startsWith('u'))
        {
            result = 'a'+word;
        }
        else 
        {
            result = 'an '+word;
        }
        return result;
        }