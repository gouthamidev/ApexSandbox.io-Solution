public Boolean ascendingOrder(Integer a, Integer b, Integer c) {
    //code here

  boolean result  ;
        integer num1 ,num2,num3 , num4;
       if ((a >= b )|| (a >= c)){
            result = true;
        }
        if ((b >= c )|| (b >= a)){
            result = true;
        }
        if ((c >= b )){
            result = false;
        }
        return result;
    }
