public static Integer arraySum(List<Integer> numbers) {
    //code here
    List < Integer > SumList = new List < Integer > ();
    movingSumList.add(numbers[0]);
    for (Integer i = 0; i < numbers.size() - 1; i++) {
        Integer sum = SumList[i] + numbers[i + 1];
        SumList.add(sum);
    }
    return sum;
}